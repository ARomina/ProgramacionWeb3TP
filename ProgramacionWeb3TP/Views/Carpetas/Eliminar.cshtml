@model ProgramacionWeb3TP.Models.Carpeta

@{
    ViewBag.Title = "Carpeta - Eliminar";
}

<div class="container">
    <div class="row profile">
        <div class="col-md-12 col-sm-12">
            <h2 class="paddingLeftTitles">Eliminar - Carpeta "@Html.DisplayFor(model => model.Nombre)"</h2>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-12 col-sm-12 text-center">


            @using (Html.BeginForm("EliminacionCarpeta", "Carpetas"))
            {
                @Html.AntiForgeryToken()
                @Html.HiddenFor(model => model.IdCarpeta)
                <h4>¿Desea eliminar esta carpeta?</h4>
                <div class="center-block"> 
                    <input type="submit" value="  Eliminar  " class="btn btn-default " /> |
                    @Html.ActionLink("  Cancelar  ", "Index", null, new { @class = "btn btn-danger" })
                </div>
            }
        </div>
    </div>
</div>

